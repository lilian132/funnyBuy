.yc .header {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  border-top: 1rpx solid #E0E7EB;
  color: #4a4a4a;
  font-size: 30rpx;
}
.yc .header view {
    width: 50%;
    text-align: center;
}
.yc .header view:nth-child(1) {
      width: 40rpx;
}
.yc .header uni-image {
    width: 30rpx;
    height: 30rpx;
    top: 1px;
    left: 2px;
}
.yc .list {
  background-color: #fff;
  border-top: 10rpx solid #E0E7EB;
  padding: 20rpx 0;
  color: #4a4a4a;
}
.yc .list .row {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    height: 60rpx;
    line-height: 60rpx;
}
.yc .list .row .col-h {
      width: 40rpx;
}
.yc .list .row .col {
      width: 50%;
      text-align: center;
}
.yc .list .row.isWin {
      color: #FF5847;
}
.yc .list .row .zl {
      width: 40rpx;
      height: 40rpx;
}
.yc .mask_layer_price {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.75);
  z-index: 992;
  opacity: 0;
  -webkit-transition: opacity ease-in 0.35s;
  -o-transition: opacity ease-in 0.35s;
  transition: opacity ease-in 0.35s;
  display: none;
}
.yc .mask_layer_price.show {
    display: block;
    opacity: 1;
}
.yc .pop_price_tips {
  position: fixed;
  width: 510rpx;
  top: 50%;
  left: 50%;
  opacity: 0;
  background-color: #fff;
  -webkit-transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  -webkit-transition: opacity ease-in 0.35s;
  -o-transition: opacity ease-in 0.35s;
  transition: opacity ease-in 0.35s;
  z-index: 993;
  border-radius: 20rpx;
}
.yc .pop_price_tips.show {
    opacity: 1;
}
.yc .pop_price_tips .title {
    padding: 30rpx 30rpx 20rpx;
    color: #000;
    text-align: center;
    font-size: 34rpx;
}
.yc .pop_price_tips .tips-wrap {
    text-align: left;
    padding: 20rpx 30rpx 60rpx;
    font-size: 26rpx;
    color: rgba(0, 0, 0, 0.85);
}
.yc .pop_price_tips .btnok {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #FF5847;
    text-align: center;
    color: #fff;
    font-size: 34rpx;
    font-weight: 500;
    letter-spacing: 1rpx;
    border-radius: 0 0 20rpx 20rpx;
}

